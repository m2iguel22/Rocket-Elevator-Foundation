                    <%= form_for @quote do |f| %>

                        <!-- form -->
                        <section class= "formStyle" id= "toPrintQuote">

                            <!-- choose type of building -->
                            <div class= "chooseType">
                                <label class= "labelStyle">Type of Building</label>

                                <select class= "ansStyle" onchange="showHide()" id= "buildingType" name="Type_of_building">
                                    <option value="">---- Type ----</option>
                                    <option value= "residential">residential</option>
                                    <option value= "commercial">commercial</option>
                                    <option value= "corporate">corporate</option>
                                    <option value= "hybride">hybride</option>
                                </select>
                            </div>
                            <!-- /choose type of building -->

                            <!-- if residential -->
                            <div class= "showHidden" id= "res1"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many apartments?</label>
                                    <input name= 'Residential_apartment_number' class= "ansStyle" type= "number" placeholder= "Apartments Number" id= "apartmentNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>

                                <div class= "askStyle">
                                    <label class= "labelStyle">How many floors?</label>
                                    <input name= 'Residential_floor_number' class= "ansStyle" type= "number" placeholder= "Floors Number" id= "resFloorNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>

                                <div class= "askStyle">
                                    <label class= "labelStyle">How many undergrounds?</label>
                                    <input name= 'Residential_underground_number' class= "ansStyle" type= "number" placeholder= "Undergrounds Number"/></br>
                                </div>

                            </div>  
                            <!-- /if residential -->

                            <!-- if commercial -->
                            <div class= "showHidden" id= "com1"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many stores?</label>
                                    <input  name= 'Commercial_store_number' class= "ansStyle" name="nb_stores" type= "number" placeholder= "Stores Number"/></br>
                                </div>

                                <div class= "askStyle">
                                    <label class= "labelStyle">How many floors?</label>
                                    <input name= 'Commercial_floor_number' class= "ansStyle" type= "number" placeholder= "Floors Number"/></br>
                                </div>

                                <div class= "askStyle">
                                    <label class= "labelStyle">How many undergrounds?</label>
                                    <input name= 'Commercial_underground_number' class= "ansStyle" type= "number" placeholder= "Undergrounds Number"/></br>
                                </div>
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many elevator cages?</label>
                                    <input name= 'Commercial_elevCage_number' class= "ansStyle" type= "number" placeholder= "Cage Number" id= "comElevCageNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many parking places?</label>
                                    <input name= 'Commercial_parkPlace_number' class= "ansStyle" type= "number" placeholder= "Parking Places"/></br>
                                </div>

                            </div> 
                            <!-- /if commercial -->

                            <!-- if corporate -->
                            <div class= "showHidden" id= "corp1"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many separate tenant companies?</label>
                                    <input name= 'Corporate_companie_number' class= "ansStyle" type= "number" placeholder= "Companies Number"/></br>
                                </div>
        
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many floors?</label>
                                    <input name= 'Corporate_floor_number' class= "ansStyle" type= "number" placeholder= "Floors Number" id= "corFloorNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>
        
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many undergrounds?</label>
                                    <input name= 'Corporate_underground_number' class= "ansStyle" type= "number" placeholder= "Undergrounds Number" id= "corUndergrNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many parking places?</label>
                                    <input name='Corporate_parkPlace_number' class= "ansStyle" type= "number" placeholder= "Parking Places"/></br>
                                </div>
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many occupants per floor?</label>
                                    <input name= 'Corporate_OccPerFloor_number' class= "ansStyle" type= "number" placeholder= "Occupants Per Floor" id= "corOccPerFloorNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>
        
                            </div> 
                            <!-- /if corporate -->

                            <!-- if hybrid -->
                            <div class= "showHidden" id= "hyb1"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many stores?</label>
                                    <input name= 'Hybrid_store_number' class= "ansStyle" type= "number" placeholder= "Stores Number"/></br>
                                </div>
        
                                <div class= "askStyle">                                    
                                    <label class= "labelStyle">How many floors?</label>
                                    <input name='Hybrid_floor_number' class= "ansStyle" type= "number" placeholder= "Floors Number" id= "hybFloorNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>
        
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many undergrounds?</label>
                                    <input name='Hybrid_underground_number' class= "ansStyle" type= "number" placeholder= "Undergrounds Number" id= "hybUndergrNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many parking places?</label>
                                    <input name= 'Hybrid_parkPlace_number' class= "ansStyle" type= "number" placeholder= "Parking Places"/></br>
                                </div>
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many occupants per floor?</label>
                                    <input name= 'Hybrid_OccPerFloor_number' class= "ansStyle" type= "number" placeholder= "Occupants Per Floor" id= "hybOccPerFloorNumber" onkeyup= "calcCageNumber(); quoteMake()"/></br>
                                </div>
                                <div class= "askStyle">
                                    <label class= "labelStyle">How many hours per day of activity?</label>
                                    <input name= 'Hybrid_hourAct_number' class= "ansStyle" type= "number" placeholder= "Open hours" id= "hoursOfActivity" onkeyup= "correctHour()"/></br>
                                </div>    
        
                            </div> 
                            <!-- /if hybrid -->

                            
                            <div id= "estCageNumber" class= "divSize"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">The estimate cage number is :</label>
                                    <input name= 'Est_cage_number'class= "ansStyle" type= "number" id= "estimateCageNumber" placeholder= "Estimate Cage Number" value disabled/></br>
                                </div>      
                            </div>
                            

                            <div class= "divSize"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">Which range of elevator cage do you need?</label>
                                    <input type= "radio" value= "standard" name= "Range" class= "radioStyle0" id= "stdRange" onclick= "quoteMake()"/><i style= "color: #fff; font-weight: bold;"> STANDARD</i>
                                    <input type= "radio" value= "premium" name= "Range" class= "radioStyle1" id= "prmRange" onclick= "quoteMake()"/><i style= "color: #fff; font-weight: bold;"> PREMIUM</i>
                                    <input type= "radio" value= "excelium" name= "Range" class= "radioStyle1" id= "excRange" onclick= "quoteMake()"/><i style= "color: #fff; font-weight: bold;"> EXCELIUM</i>

                                </div>      
                            </div>
                
                            
                            <div class= "divSize"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">Here is your quote :</label>
                                    <div class= "quoteStyle">
                                        <label class = "quoteLabelStyle">Unit Price :</label><label class = "quoteLabelStyle" id= "unitP"></label> </br>
                                    </div>
                                    <div class= "quoteStyle">
                                        <label class = "quoteLabelStyle">Elevator Price :</label><label class = "quoteLabelStyle" id= "elevP"></label> </br>
                                    </div>
                                    <div class= "quoteStyle">
                                        <label class = "quoteLabelStyle">Installation Cost :</label><label class = "quoteLabelStyle" id= "instP"></label> </br>
                                    </div>
                                    <div class= "quoteStyle">
                                        <label class = "quoteLabelStyle">Total Price :</label><label class = "quoteLabelStyle" id= "totalP"></label></br>
                                    </div>
                                </div>      
                            </div>



                            <div class= "divSize"> 
                                <div class= "askStyle">
                                    <label class= "labelStyle">Clients information</label>
                                    <div class= "quoteStyle">
                                        <label class = "quoteLabelStyle">Your Full Name:</label>
                                        <input name='Name' class= "ansStyle" required type="text"/></br>
                                        <label class = "quoteLabelStyle">Email Address:</label>
                                        <input name='Email' class= "ansStyle" required type="email"/></br>
                                        <label class = "quoteLabelStyle">Phone Number:</label>
                                        <input name='Phone' class= "ansStyle" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}" placeholder="000-000-0000" type= "tel"/></br>
                                    </div>
                                
                                </div>      
                            </div>

                            
                                <input name= 'Unit_price' id= "A" style= 'display: none'/>
                                <input name= 'Elevator_price' id= "B" style= 'display: none'/>
                                <input name= 'Installation_cost' id= "C" style= 'display: none'/>
                                <input name= 'Total_price' id= "D" style= 'display: none'/>

                            
                            

                            

                            <br>




                            
                            <button class="btn btn-green btn-lg" type= 'submit'>SUBMIT QUOTE</button>
                            

                        </section>
                        <!-- /form -->
                    <% end %>




                    
            
